	public void call1(String nos) {
		Intent callIntent = new Intent(Intent.ACTION_DIAL);
		callIntent.setData(Uri.parse("tel:" + nos));
		startActivity(callIntent);
	}

	class Registrationasyn extends AsyncTask<Void, Integer, Void> {

		@Override
		protected void onPreExecute() {
			mProgressDialog = new ProgressDialog(MainActivity.this);
			mProgressDialog.setTitle("Loading...");
			mProgressDialog.show();

		}

		@Override
		protected Void doInBackground(Void... params) {

			try {
				restUrl = "name="
						+ URLEncoder.encode(editname.getText().toString()
								.trim(), "UTF-8")
						+ "&dob="
						+ URLEncoder.encode(
								editdob.getText().toString().trim(), "UTF-8")
						+ "&nationality="
						+ URLEncoder.encode(editnationality.getText()
								.toString().trim(), "UTF-8")
						+ "&passport_no="
						+ URLEncoder.encode(editpassportnum.getText()
								.toString().trim(), "UTF-8")
						+ "&email="
						+ URLEncoder.encode(editemail.getText().toString()
								.trim(), "UTF-8");

			} catch (Exception e) {
				// TODO: handle exception
			}

			try {
				HttpClient client = new DefaultHttpClient();
				HttpGet request = new HttpGet(
						"http://178.62.43.79/cgiapp/api/register.php?"
								+ restUrl); // replace
				Log.e("Name : ", "Name : " + restUrl);

				try {
					response = client.execute(request);
					HttpEntity entity = response.getEntity();

					is = entity.getContent();

					try {
						BufferedReader reader = new BufferedReader(
								new InputStreamReader(is, "iso-8859-1"), 8);
						StringBuilder sb = new StringBuilder();
						String line = null;
						while ((line = reader.readLine()) != null) {
							sb.append(line + "\n");
						}
						is.close();
						json = sb.toString();

					} catch (Exception e) {
						Log.e("Buffer Error",
								"Error converting result " + e.toString());
					}

					Log.e("DATA: ", "PARSING DATA " + json);

					// Toast.LENGTH_LONG).show();
				} catch (ClientProtocolException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				} catch (IOException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
			} catch (NullPointerException e) {
				// TODO: handle exception
			}

			return null;
		}

		@SuppressLint("NewApi")
		@Override
		protected void onPostExecute(Void result) {
			super.onPostExecute(result);
			Log.e("Json : ", json);

			if (json.startsWith("Succsesfull")) {

				Toast.makeText(getApplicationContext(),
						"Registered Successfully", Toast.LENGTH_LONG).show();
				r1.removeAllViews();
				rl = (ViewGroup) getLayoutInflater().inflate(
						R.layout.message_from_cg_tab, null);

				WebView message_consolate = (WebView) rl
						.findViewById(R.id.message_of_cg);
				message_consolate
						.loadUrl("file:///android_asset/cgihtml/welcome.html");

				r1.addView(rl);
			} else {
				Toast.makeText(getApplicationContext(), json, Toast.LENGTH_LONG)
						.show();
			}
			mProgressDialog.dismiss();

		}
	}

	private DatePickerDialog.OnDateSetListener mDateSetListener = new DatePickerDialog.OnDateSetListener() {

		public void onDateSet(DatePicker view, int year, int monthOfYear,
				int dayOfMonth) {
			mYear = year;
			mMonth = monthOfYear;
			mDay = dayOfMonth;

			SimpleDateFormat sdf = new SimpleDateFormat("dd/MM/yyyy");
			Date date1 = null;
			Date date2 = null;
			String date1s = sdf.format(c.getTime());
			try {
				date1 = sdf.parse(date1s);
			} catch (ParseException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}

			String str2 = Integer.toString(mDay) + "/"
					+ Integer.toString(mMonth + 1) + "/"
					+ Integer.toString(mYear);
			try {
				date2 = sdf.parse(str2);
			} catch (ParseException e) {
				e.printStackTrace();
			}
			if (date1.compareTo(date2) < 0) {
				Toast.makeText(getApplicationContext(),
						"Future date is not allowed", Toast.LENGTH_SHORT)
						.show();
			} else {
				editdob.setText(new StringBuilder().append(mDay).append("/")
						.append(mMonth + 1).append("/").append(mYear));
			}

		}

	};

	protected Dialog onCreateDialog(int id) {
		switch (id) {
		case DATE_DIALOG_ID:
			DatePickerDialog dialog = new DatePickerDialog(this,
					mDateSetListener, mYear, mMonth, mDay);

			return dialog;

		}

		return null;

	}

	public void showAlertDialog(Context context, String title, String message,
			Boolean status) {
		AlertDialog alertDialog = new AlertDialog.Builder(context).create();

		// Setting Dialog Title
		alertDialog.setTitle(title);

		// Setting Dialog Message
		alertDialog.setMessage(message);

		// Setting alert dialog icon

		// Setting OK Button
		alertDialog.setButton("OK", new DialogInterface.OnClickListener() {
			public void onClick(DialogInterface dialog, int which) {
			}
		});

		// Showing Alert Message
		alertDialog.show();
	}

	private class MyBrowser extends WebViewClient {

		@Override
		public void onPageStarted(WebView view, String url, Bitmap favicon) {
			// TODO Auto-generated method stub
			super.onPageStarted(view, url, favicon);

		}

		@Override
		public boolean shouldOverrideUrlLoading(WebView view, String url) {

			Log.e("URLS", url);
			// Toast.makeText(getApplicationContext(), url, Toast.LENGTH_LONG);
			if (url.equalsIgnoreCase("file:///android_asset/cgihtml/register")) {

				r1.removeAllViews();
				rl = (ViewGroup) getLayoutInflater().inflate(
						R.layout.registration_tab, null);
				editname = (EditText) rl.findViewById(R.id.fullname);
				editdob = (EditText) rl.findViewById(R.id.dob);
				editnationality = (EditText) rl.findViewById(R.id.nationality);
				editpassportnum = (EditText) rl.findViewById(R.id.passport);
				editemail = (EditText) rl.findViewById(R.id.email);

				subitimage = (ImageView) rl
						.findViewById(R.id.download_form_button);
				c = Calendar.getInstance();
				mYear = c.get(Calendar.YEAR);
				mMonth = c.get(Calendar.MONTH);
				mDay = c.get(Calendar.DAY_OF_MONTH);

				SimpleDateFormat sdf = new SimpleDateFormat("dd/MM/yyyy");
				String asa = sdf.format(c.getTime());
				dcheck = asa.substring(0, 2);

				mcheck = asa.substring(3, 5);

				ycheck = asa.substring(6);

				editdob.setOnClickListener(new OnClickListener() {

					@Override
					public void onClick(View v) {
						// TODO Auto-generated method stub
						showDialog(DATE_DIALOG_ID);

					}
				});
				subitimage.setOnClickListener(new OnClickListener() {

					@Override
					public void onClick(View v) {
						// TODO Auto-generated method stub

						cd = new ConnectionDetector(getApplicationContext());
						// get Internet status
						isInternetPresent = cd.isConnectingToInternet();

						if (isInternetPresent) {

							if (editname.getText().toString().trim().equals("")
									|| editdob.getText().toString().trim()
											.equals("")
									|| editnationality.getText().toString()
											.trim().equals("")
									|| editpassportnum.getText().toString()
											.trim().equals("")
									|| editemail.getText().toString().trim()
											.equals("")) {
								Toast.makeText(getApplicationContext(),
										"Please fill all details properly",
										Toast.LENGTH_SHORT).show();

							} else {

								emailPattern = emailPattern(editemail.getText()
										.toString());

								if (emailPattern) {
									Obj_Registrationasyn = new Registrationasyn();
									Obj_Registrationasyn.execute();

								} else {
									Toast.makeText(getApplicationContext(),
											"Please Enter Valid Email",
											Toast.LENGTH_SHORT).show();

								}

							}

						}

						else {

							Toast.makeText(getApplicationContext(),
									"Please Check Your Internet Connection",
									Toast.LENGTH_SHORT).show();

						}

					}
				});
				sos = (ImageButton) rl.findViewById(R.id.imageButton1);
				sos.setOnClickListener(new OnClickListener() {

					@Override
					public void onClick(View v) {
						// TODO Auto-generated method stub
						call1("0097180046342");
					}
				});
				r1.addView(rl);
			}

			return true;

		}

	}

	public static boolean emailPattern(String email) {
		boolean isValid = false;

		String expression = "^[\\w\\.-]+@([\\w\\-]+\\.)+[A-Z]{2,4}$";
		CharSequence inputStr = email;

		Pattern pattern = Pattern.compile(expression, Pattern.CASE_INSENSITIVE);
		Matcher matcher = pattern.matcher(inputStr);
		if (matcher.matches()) {
			isValid = true;
		}
		return isValid;
	}

	public void Is_Valid_Passport(EditText edt) throws NumberFormatException {
		if (edt.getText().toString().length() <= 0) {
			validName = null;
		} else if (!edt.getText().toString().matches("[a-zA-Z1234567890]+")) {
			// edt.setError("Accept Alphabets Only.");
			edt.setHint("PASSPORT NO");
			edt.setText("");
			edt.setHintTextColor(Color.parseColor("#ff0000"));
			validName = null;
		} else {
			validName = edt.getText().toString();
			edt.setError(null);

		}

	}

	public void Is_Valid_Middle_Name(EditText edt) throws NumberFormatException {
		if (edt.getText().toString().length() <= 0) {
			validName = null;
		} else if (!edt.getText().toString().matches("[a-zA-Z-_]+")) {
			// edt.setError("Accept Alphabets Only.");
			edt.setHint("NATIONALITY");
			edt.setText("");
			edt.setHintTextColor(Color.parseColor("#ff0000"));
			validName = null;
		} else {
			validName = edt.getText().toString();
			edt.setError(null);

		}

	}

	// String specialChars = "/*!@#$%^&*()\"{}_[]|\\?/<>,.";
	public void Is_Valid_First_Name(EditText edt) throws NumberFormatException {
		if (edt.getText().toString().length() <= 0) {
			validName = null;
		} else if (!edt.getText().toString().matches("[a-zA-Z. ]+")) {
			// edt.setError("Accept Alphabets Only.");
			edt.setHint("NAME");
			edt.setText("");
			edt.setHintTextColor(Color.parseColor("#ff0000"));
			validName = null;
		} else {
			validName = edt.getText().toString();
			edt.setError(null);

		}

	}

	class Griviencesasync extends AsyncTask<Void, Integer, Void> {

		@Override
		protected void onPreExecute() {
			mProgressDialog = new ProgressDialog(MainActivity.this);
			mProgressDialog.setTitle("Loading...");
			mProgressDialog.show();

		}

		@Override
		protected Void doInBackground(Void... params) {
			try {
				restUrl = "name="
						+ URLEncoder.encode(gr_editname.getText().toString()
								.trim(), "UTF-8")
						+ "&text="
						+ URLEncoder.encode(
								gr_text.getText().toString().trim(), "UTF-8")
						+ "&mobile="
						+ URLEncoder.encode(gr_editmoblenumber.getText()
								.toString().trim(), "UTF-8")
						+ "&subject="
						+ URLEncoder.encode(gr_selecttext.getText().toString()
								.trim(), "UTF-8")
						+ "&passport_no="
						+ URLEncoder.encode(gr_editpassportnum.getText()
								.toString().trim(), "UTF-8")
						+ "&email="
						+ URLEncoder.encode(gr_editemail.getText().toString()
								.trim(), "UTF-8");

			} catch (Exception e) {
				// TODO: handle exception
			}

			try {
				HttpClient client = new DefaultHttpClient();
				HttpGet request = new HttpGet(
						"http://apps.eadaapps.com/cgi_dubai/api/register.php?"
								+ restUrl); // replace
				Log.e("Name : ", "Name : " + restUrl);

				try {
					response = client.execute(request);
					HttpEntity entity = response.getEntity();

					is = entity.getContent();

					try {
						BufferedReader reader = new BufferedReader(
								new InputStreamReader(is, "iso-8859-1"), 8);
						StringBuilder sb = new StringBuilder();
						String line = null;
						while ((line = reader.readLine()) != null) {
							sb.append(line + "\n");
						}
						is.close();
						json = sb.toString();

					} catch (Exception e) {
						Log.e("Buffer Error",
								"Error converting result " + e.toString());
					}

					Log.e("DATA: ", "PARSING DATA " + json);

					// Toast.LENGTH_LONG).show();
				} catch (ClientProtocolException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				} catch (IOException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
			} catch (NullPointerException e) {
				// TODO: handle exception
			}

			return null;
		}

